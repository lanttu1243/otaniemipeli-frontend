{
  "name": "otaniemipeli-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "lint-fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,ts,tsx,md,json}\"",
    "clean": "rm -rf .next node_modules && pnpm install",
    "pre-commit": "pnpm clean && pnpm lint && pnpm typecheck && pnpm format && git add . && echo 'Pre-commit checks completed!'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "lucide-react": "^0.483.0",
    "next": "15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.3.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
